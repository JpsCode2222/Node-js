<form action="/save_feedback" method="post">
    Visitor Name*
    <input type="text" required name="visitor_name"><br><br>
    Customer Name* 
    <input type="text" required name="customer_name"><br><br>
    Customer Mobile*
    <input type="text" required name="customer_mobile"><br><br>
    Customer email
    <input type="text" name="customer_email"><br><br>
    Customer Message* 
    <textarea required name="customer_message"></textarea><br><br>
    <button>Save Feedback</button>
</form>

<%=fd%>
<h1>Feedback List</h1>
<table border="1">
    <tr>
        <th>Edit</th>
        <th>SN</th>
        <th>Visitor Name</th>
        <th>Customer Name</th>
        <th>Customer Mobile</th>
        <th>Customer Email</th>
        <th>Customer Feedback</th>
    </tr>
    <% for(var i=0; i<fd.length; i++){%>
        <tr>
            <td>
                <a href="/delete_feedback?id=<%=fd[i].feedback_id%>">
                    <button onclick="return confirm('are you sure...')">Delete</button>
                </a>
                <a href="/edit_feedback?id=<%=fd[i].feedback_id%>">
                    <button onclick="return confirm('are you sure...')">Delete</button>
                </a>
            </td>
            <td><%=i+1%></td>
            <td><%=fd[i].visitor_name%></td>
            <td><%=fd[i].customer_name%></td>
            <td><%=fd[i].customer_mobile%></td>
            <td><%=fd[i].customer_email%></td>
            <td><%=fd[i].customer_message%></td>
        </tr>
        <%}%>
</table>