<h1 style="text-align-last: center;">DATA</h1>
<table border="1" width="100%">
    <tr>
        <th>Sr No.</th>
        <th>Name</th>
        <th>Mobile</th>
        <th>Email</th>
    </tr>

    <% 
    for(var i=0;i<data.length;i++){
        %>
        <tr style="text-align:center">
            <td><%= i+1 %></td>
            <td><%= data[i].name %></td>
            <td><%= data[i].mobile %></td>
            <td><%= data[i].email %></td>
        </tr>
        <%
    }
    %>
</table>
